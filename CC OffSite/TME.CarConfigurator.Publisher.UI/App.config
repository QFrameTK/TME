<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
  </configSections>
  <appSettings file="AWSSettings.config">
    <add key="Environment" value="development" />
  </appSettings>
  <spring>
    <context>
      <resource uri="config://spring/objects" />
    </context>
    <objects xmlns="http://www.springframework.net">
      <object name="ContextFactory" type="TME.CarConfigurator.Publisher.Factories.ContextFactory, TME.CarConfigurator.Publisher"></object>
      <object name="PublisherFactory" type="TME.CarConfigurator.Publisher.Factories.PublisherFactory, TME.CarConfigurator.Publisher"></object>
      <object name="Publisher" type="TME.CarConfigurator.Publisher.S3.S3Publisher, TME.CarConfigurator.Publisher">
        <constructor-arg ref="S3PublicationService" />
        <constructor-arg ref="S3LanguageService" />
        <constructor-arg ref="S3BodyTypeService" />
      </object>
      <object name="ServiceFactory" type="TME.CarConfigurator.Publisher.Factories.ServiceFactory, TME.CarConfigurator.Publisher"></object>
      <object name="Mapper" type="TME.CarConfigurator.Publisher.Mapper, TME.CarConfigurator.Publisher"></object>
      <object name="CarDbModelGenerationFinder" type="TME.CarConfigurator.Publisher.CarDbModelGenerationFinder, TME.CarConfigurator.Publisher"></object>
      <object name="PublicationService" type="TME.CarConfigurator.Publisher.PublicationService, TME.CarConfigurator.Publisher">
        <constructor-arg ref="ContextFactory" />
        <constructor-arg ref="PublisherFactory" />
        <constructor-arg ref="ServiceFactory" />
        <constructor-arg ref="Mapper" />
        <constructor-arg ref="CarDbModelGenerationFinder" />
      </object>
      <object name="S3Serialiser" type="TME.CarConfigurator.Publisher.S3.S3Serialiser, TME.CarConfigurator.Publisher"></object>
      <object name="S3Service" type="TME.CarConfigurator.Publisher.S3.S3Service, TME.CarConfigurator.Publisher">
        <constructor-arg index="0" type="String" value="fakeBrand" />
        <constructor-arg index="1" type="String" value="fakeCountry" />
        <constructor-arg index="2" ref="AmazonS3" />
      </object>
      <object name="S3PublicationService" type="TME.CarConfigurator.Publisher.S3.S3PublicationService, TME.CarConfigurator.Publisher">
        <constructor-arg ref="S3Service" />
        <constructor-arg ref="S3Serialiser" />
      </object>
      <object name="S3LanguageService" type="TME.CarConfigurator.Publisher.S3.S3LanguageService, TME.CarConfigurator.Publisher">
        <constructor-arg ref="S3Service" />
        <constructor-arg ref="S3Serialiser" />
      </object>
      <object name="S3BodyTypeService" type="TME.CarConfigurator.Publisher.S3.S3BodyTypeService, TME.CarConfigurator.Publisher">
        <constructor-arg ref="S3Service" />
        <constructor-arg ref="S3Serialiser" />
      </object>
      <object name="AmazonS3Factory" type="TME.CarConfigurator.Publisher.Factories.AmazonS3Factory, TME.CarConfigurator.Publisher"></object>
      <object name="AmazonS3" factory-method="CreateInstance" factory-object="AmazonS3Factory"></object>
      <object name="ShellViewModel" type="TME.CarConfigurator.Publisher.UI.ViewModels.ShellViewModel, TME.CarConfigurator.Publisher.UI" autowire="byType" />
      <!--<object name="ShellViewModel" type="TME.CarConfigurator.Publisher.UI.ViewModels.ShellViewModel, TME.CarConfigurator.Publisher.UI">
        <constructor-arg index="0" ref="PublicationService"/>
      </object>-->
    </objects>
  </spring>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="TME.BusinessObjects" publicKeyToken="e6963f4af1c2f0e2" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.11.0" newVersion="4.0.11.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="TMME.Security.Library" publicKeyToken="e6963f4af1c2f0e2" />
        <bindingRedirect oldVersion="0.0.0.0-1.3.0.0" newVersion="1.3.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="TME.Common" publicKeyToken="e6963f4af1c2f0e2" />
        <bindingRedirect oldVersion="0.0.0.0-1.5.0.0" newVersion="1.5.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="TMME.Common" publicKeyToken="e6963f4af1c2f0e2" />
        <bindingRedirect oldVersion="0.0.0.0-1.3.0.0" newVersion="1.3.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>