@model TME.FrontEndViewer.Models.CompareView<TME.CarConfigurator.Interfaces.IGrade>
<h2>Toyota Viewer</h2>
<div>
    <table>
        <tr>
            <td id="oldarea" class="areaview">@Html.Partial("Grades/details",Model.OldReaderModel.Model)</td>
            <td id="newarea" class="areaview">@Html.Partial("Grades/details",Model.NewReaderModel.Model)</td>
        </tr>
        <tr>
            <td>@Html.Raw(Model.OldReaderModel.TimeToLoad.TotalMinutes)</td>
            <td>@Html.Raw(Model.NewReaderModel.TimeToLoad.TotalMinutes)</td>
        </tr>
    </table>
</div>
