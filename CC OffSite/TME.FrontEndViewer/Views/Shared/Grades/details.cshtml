@model IEnumerable<TME.CarConfigurator.Interfaces.IGrade>


@foreach (var model in Model)
{
    <div class="clickable">
        <table class="top">
            <tr>
                <td>
                    <b>@Html.Raw(model.Name)</b>
                </td>
            </tr>
        </table>
        @Html.Partial("_grade", model)
    </div>
}