@model IList<TME.CarConfigurator.Interfaces.IModel>


@foreach (var model in Model)
{
    <div class="clickable">
        <table class="top">
            <tr>
                <td>
                    <b>@Html.Raw(model.Name)</b>
                </td>
            </tr>
        </table>
        <div class="clickable">
            Description : @Html.Raw(model.Description)<br/>
            ToolTip : @Html.Raw(model.ToolTip)<br/>
            FootNote : @Html.Raw(model.FootNote)<br/>
            <br/>
            <a href="">Assets</a> | <a href="">Cars</a> 
            <br/> <br/>
        </div>
    </div>
}